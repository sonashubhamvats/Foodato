<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminOrderOutForDelivery"
    android:background="@drawable/background">
    <TextView
        android:id="@+id/textView13"
        android:layout_width="300sp"
        android:layout_height="116dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="95dp"
        android:text="Orders out for delivery"
        android:textColor="#fff"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/adminOrderOutForDeliveryPrompt"
        android:layout_width="360dp"
        android:layout_height="66dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="33dp"
        android:text="Nothing to show here!!"
        android:textColor="#ff4969"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <ListView
        android:id="@+id/adminlistofOrderOutForDelivery"
        android:layout_width="355dp"
        android:layout_height="252dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="33dp"
        android:background="@drawable/custom_listview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminOrderOutForDeliveryPrompt" />

    <Button
        android:id="@+id/adminOrderOutForDelivery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="288dp"
        android:layout_marginTop="33dp"
        android:textColor="#ffff"
        android:textAllCaps="false"
        android:background="@drawable/custom_button"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="96dp"
        android:onClick="RefreshButtonofd"
        android:text="Refresh"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adminlistofOrderOutForDelivery" />

</androidx.constraintlayout.widget.ConstraintLayout>